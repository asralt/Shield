{
  "expo": {
    "name": "my-expo-app",
    "slug": "my-expo-app",
    "scheme": "myexpoapp",
    "version": "1.0.0",
    "icon": "./assets/icon.png",
    "web": {},
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-router"
    ],
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#001845"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.asraltenkhbadral.myexpoapp",
      "supportsTablet": true,
      "infoPlist": {
        "NSLocalNetworkUsageDescription": "This app needs access to the local network to scan for devices.",
        "NSAppTransportSecurity": {
          "NSAllowsLocalNetworking": true
        },
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.asraltenkhbadral.myexpoapp",
      "adaptiveIcon": {
        "backgroundColor": "#001845"
      }
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "792776b5-b737-4a1a-89e1-719c991a3056"
      }
    }
  }
}
